{
  "name": "serverless-boilerplate",
  "description": "This template demonstrates how to make a simple HTTP API with Node.js and Typescript running on AWS Lambda and API Gateway using the Serverless Framework v3",
  "version": "1.0.0",
  "main": "handler.ts",
  "scripts": {
    "sls": "serverless",
    "build": "pnpm sls package --aws-profile breakingmaurin",
    "type-check": "echo \"Type-Check not implemented yet\"",
    "test-unity": "echo \"Test-Unity not implemented yet\"",
    "deploy:dev": "pnpm sls deploy --aws-profile breakingmaurin --stage dev",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@serverless/typescript": "^3.25.0",
    "@types/aws-lambda": "^8.10.101",
    "@types/node": "^18.11.18",
    "esbuild": "^0.16.13",
    "husky": "^8.0.3",
    "semantic-release": "^19.0.5",
    "serverless": "^3.26.0",
    "serverless-esbuild": "^1.35.3",
    "serverless-offline": "^12.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.7.4"
  }
}
