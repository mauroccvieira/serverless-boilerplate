{
  "name": "serverless-boilerplate",
  "description": "This template demonstrates how to make a simple HTTP API with Node.js and Typescript running on AWS Lambda and API Gateway using the Serverless Framework v3",
  "version": "1.0.0",
  "main": "handler.ts",
  "scripts": {
    "sls": "serverless",
    "build": "yarn sls package --aws-profile breakingmaurin",
    "type-check": "echo \"Type-Check not implemented yet\"",
    "test:unity": "jest",
    "deploy:dev": "yarn sls deploy --aws-profile breakingmaurin --stage dev",
    "lint": "eslint ./src --fix --cache",
    "lint:tests": "eslint ./__tests__ --fix --cache",
    "format": "prettier ./src --write",
    "format:tests": "prettier ./__tests__ --write",
    "prepare": "husky install",
    "test": "jest"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@serverless/typescript": "^3.25.0",
    "@types/aws-lambda": "^8.10.101",
    "@types/jest": "^29.2.6",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "esbuild": "^0.16.13",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-simple-import-sort": "^9.0.0",
    "eslint-plugin-sonarjs": "^0.18.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "semantic-release": "^19.0.5",
    "serverless": "^3.26.0",
    "serverless-esbuild": "^1.35.3",
    "serverless-offline": "^12.0.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.7.4"
  }
}
